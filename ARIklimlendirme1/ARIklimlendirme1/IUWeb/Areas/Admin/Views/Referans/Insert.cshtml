@model DataAccsess.Models.Referance

<div class="container">
    <form action="/Admin/Referans/Insert" method="post" enctype="multipart/form-data">
    <input type="file" class="form-control" style="margin-bottom:10px;" name="file"/>
    <input type="text" class="form-control" style="margin-bottom:10px;" placeholder="Referans Adı" asp-for="ReferanceName"/>
    <button type="submit" class="form-control btn btn-primary">Ekleme Yap</button>
    </form>
</div>

@if(ViewBag.Message != null)
{
    <div class="alert alert-success" style="margin-top:15px">@ViewBag.Message</div>

}
@if(ViewBag.Error != null )
{
    <div class="alert alert-danger" style="margin-top:15px">@ViewBag.Error</div>
}